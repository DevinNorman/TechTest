        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TechTest"
        ResizeMode="NoResize"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="530">
  <Grid Margin="0,0,0,0">
