    <TextBlock Name="Result" HorizontalAlignment="Left" Margin="23,59,0,0" TextWrapping="Wrap" Text="Output" VerticalAlignment="Top" Width="470" Grid.ColumnSpan="2"/>
    <TextBlock Name="Computations" HorizontalAlignment="Left" Margin="23,209,0,0" TextWrapping="Wrap" Text="Processing time" VerticalAlignment="Top" Width="476" Grid.ColumnSpan="2"/>

  </Grid>
</Window>
